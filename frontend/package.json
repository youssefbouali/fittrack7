{
  "name": "fittrack-frontend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build && next export",
    "start": "next start",
    "export": "next build && next export",
    "deploy:s3": "next build && next export && aws s3 sync out s3://$AWS_S3_BUCKET --delete",
    "deploy:invalidate": "aws cloudfront create-invalidation --distribution-id $AWS_CLOUDFRONT_DISTRIBUTION_ID --paths '/*'"
  },
  "dependencies": {
    "next": "13.5.7",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "@reduxjs/toolkit": "^1.9.7",
    "react-redux": "^8.1.3",
    "aws-amplify": "^6.0.0",
    "@aws-amplify/ui-react": "^5.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@types/node": "^20.10.0",
    "typescript": "^5.3.3"
  }
}
